{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "spring-boot-demo",
      "image": "tchao98/spring-boot-demo:latest",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "SPRING_DATASOURCE_URL",
          "value": "jdbc:postgresql://[RDS-Database-endpoint]:5432/customer"
        }
      ]
    }
  ]
}